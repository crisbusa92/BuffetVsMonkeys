    "def monkey_returns(DF,diclist):\n",
    "    returns = []\n",
    "    for x in diclist:\n",
    "        #Obtaining Weights\n",
    "        suma = 0\n",
    "        for y in x.keys():\n",
    "            #Sum \n",
    "            suma += float(DF[DF['Ticker'] == y]['5yr Return']) * x[y]\n",
    "        suma = suma/len(x.keys())\n",
    "        returns.append(suma)\n",
    "\n",
    "    return returns\n"
